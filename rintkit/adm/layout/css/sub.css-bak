.rt_thumb_view_ico_wrap{position:relative;margin-right:5px;}
.rt_thumb_view_ico_wrap .rt_thumb_view_ico{margin-right:5px;display:block;width:34px;height:34px;line-height:34px;text-align:center;background-color:#f6f6f6;border-radius:6px;-moz-border-radius:6px;-webkit-border-radius:6px;}
.rt_thumb_view_ico_wrap .rt_thumb_view_ico img{vertical-align:middle;}
.rt_thumb_view_ico_wrap .rt_thumb_view_con{display:none;position:absolute;left:100%;top:0;z-index:9999;}
.rt_thumb_view_ico_wrap:hover .rt_thumb_view_con{display:block;}